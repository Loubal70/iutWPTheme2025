{
  "$schema": "https://schemas.wp.org/wp/6.2/theme.json",
  "version": 2,
  "customTemplates": [],
  "settings": {
    "appearanceTools": true,
    "shadow": {
      "presets": [
        {
          "name": "Natural",
          "slug": "natural",
          "shadow": "6px 6px 9px rgba(0, 0, 0, 0.2)"
        },
        {
          "name": "Crisp",
          "slug": "crisp",
          "shadow": "6px 6px 0px rgba(0, 0, 0, 1)"
        }
      ]
    },
    "color": {
      "defaultGradients": false,
      "defaultPalette": false,
      "palette": [
        {
          "slug": "primary",
          "color": "#185CFF",
          "name": "Primary"
        },
        {
          "slug": "secondary",
          "color": "#002B4E",
          "name": "Secondaire"
        },
        {
          "slug": "tertiary",
          "color": "#F0F9FF",
          "name": "Tertiaire"
        },
        {
          "slug": "blue",
          "color": "#194060",
          "name": "Bleu"
        },
        {
          "slug": "blue2",
          "color": "#607D94",
          "name": "Bleu 2"
        },
        {
          "slug": "blue3",
          "color": "#C9DCEC",
          "name": "Bleu 3"
        },
        {
          "slug": "black-dark",
          "color": "#292D33",
          "name": "Black Dark"
        },
        {
          "slug": "gray",
          "color": "#9497A1",
          "name": "Gris"
        },
        {
          "slug": "gray-light",
          "color": "#F1F6FA",
          "name": "Gris clair"
        },
        {
          "slug": "rederror",
          "color": "#FF4D4D",
          "name": "Rouge"
        }
      ]
    },
    "custom": {
      "typography": {
        "font-size": {
          "xxl": "clamp(1.2rem, 4vw, 1.375rem)",
          "4xxl": "clamp(2rem, 4vw, 2.5rem)",
          "6xxl": "clamp(3.75rem, 6vw, 4.375rem)",
          "10xl": "clamp(8.75rem, 8vw, 11.25rem)"
        },
        "line-height": {
          "tiny": 1.15,
          "small": 1.2,
          "medium": 1.4,
          "normal": 1.6
        }
      }
    },
    "spacing": {
      "units": ["%", "px", "em", "rem", "vh", "vw"]
    },
    "typography": {
      "dropCap": false,
      "fontFamilies": [
        {
          "fontFamily": "Poppins, sans-serif",
          "name": "Poppins",
          "slug": "poppins",
          "fontFace": [
            {
              "fontFamily": "Poppins",
              "fontWeight": "300",
              "fontStyle": "normal",
              "fontDisplay": "swap",
              "fontStretch": "normal",
              "src": ["file:./assets/fonts/Poppins/Poppins-Light.ttf"]
            },
            {
              "fontFamily": "Poppins",
              "fontWeight": "400",
              "fontStyle": "normal",
              "fontDisplay": "swap",
              "fontStretch": "normal",
              "src": ["file:./assets/fonts/Poppins/Poppins-Regular.ttf"]
            },
            {
              "fontFamily": "Poppins",
              "fontWeight": "500",
              "fontStyle": "normal",
              "fontDisplay": "swap",
              "fontStretch": "normal",
              "src": ["file:./assets/fonts/Poppins/Poppins-Medium.ttf"]
            },
            {
              "fontFamily": "Poppins",
              "fontWeight": "600",
              "fontStyle": "normal",
              "fontDisplay": "swap",
              "fontStretch": "normal",
              "src": ["file:./assets/fonts/Poppins/Poppins-SemiBold.ttf"]
            },
            {
              "fontFamily": "Poppins",
              "fontWeight": "700",
              "fontStyle": "normal",
              "fontDisplay": "swap",
              "fontStretch": "normal",
              "src": ["file:./assets/fonts/Poppins/Poppins-Bold.ttf"]
            }
          ]
        }
      ],
      "fontSizes": [
        {
          "size": "1rem",
          "slug": "small"
        },
        {
          "size": "clamp(0.875rem, 3vw, 1rem)",
          "slug": "x-small"
        },
        {
          "size": "clamp(1.2rem, 4vw, 1.375rem)",
          "slug": "medium"
        },
        {
          "size": "clamp(1.375rem, 4vw, 1.5rem)",
          "slug": "large"
        },
        {
          "size": "clamp(1.375rem, 4vw, 1.875rem)",
          "slug": "xs-large"
        },
        {
          "size": "clamp(1.125rem, 4vw, 2.5rem)",
          "slug": "sm-large"
        },
        {
          "size": "clamp(1.4rem, 4vw, 2.5rem)",
          "slug": "x-large"
        },
        {
          "size": "clamp(1.4rem, 4vw, 3.25rem)",
          "slug": "xx-large"
        },
        {
          "size": "clamp(2.25rem, 8vw, 4.375rem)",
          "slug": "xxx-large"
        },
        {
          "size": "clamp(2.8rem, 8vw, 6.375rem)",
          "slug": "xxxs-large"
        },
        {
          "size": "clamp(3rem, 8vw, 8.75rem)",
          "slug": "xxxx-large"
        }
      ]
    },
    "box-shadow": {
      "default": "0px 0px 10px rgba(0, 0, 0, 0.1)",
      "button": "0px 4px 6px rgba(0, 0, 0, 0.1)"
    },
    "layout": {
      "contentSize": "1460px",
      "wideSize": "1460px"
    }
  },
  "styles": {
    "elements": {
      "h1": {
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--poppins)",
          "fontWeight": "600"
        }
      },
      "h2": {
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--poppins)",
          "fontWeight": "600"
        }
      },
      "link": {
        "color": {
          "text": "var(--wp--preset--color--primary)"
        }
      }
    },
    "spacing": {
      "blockGap": "1.5rem"
    },
    "typography": {
      "fontFamily": "var(--wp--preset--font-family--poppins)",
      "lineHeight": "var(--wp--custom--typography--line-height--normal)",
      "fontSize": "var(--wp--preset--font-size--small)"
    }
  },
  "templateParts": []
}